doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/urlList.css')
  body
    ul.url-list
      li.url-list-item
        span.url-list-title 接口名称
        span.url-list-url url
        span.url-list-server 外部url
      each item in urlList
        li.url-list-item
          a.url-list-a(href='./urlItem/' + item.id)
            span.url-list-title #{item.title}
            span.url-list-url #{item.url}
            span.url-list-server #{item.serverUrl}

